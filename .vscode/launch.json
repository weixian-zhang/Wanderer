    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Dolos",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src/apps/dolos",
            "env": {
                "AzDiskMountFilePath": "/mnt/azdisk/dolos.txt",
                "PORT": "8000",
                "MONGOCONNSTRING": ""
            },
            "args": []
        },
        {
            "name": "Debug Ymir",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src/apps/ymir",
            "env": {
               "PORT": "8010",
               "BOOKAPIBASEURL": "http://localhost:8000"
            },
            "args": []
        }
        

    ]
}